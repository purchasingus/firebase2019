<?xml version='1.0' encoding='utf-8'?>
<widget id="my.chancornerstone.firebase2019" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Firebase2019</name>
  <description>
      firebase test 2019 dec
  </description>
  <author email="dev@cordova.apache.org" href="http://cordova.io">
      Apache Cordova Team
  </author>
  <content src="index.html" />
  
  <preference name="phonegap-version" value="cli-9.0.0" />
  <preference name="permissions" value="none" />
  <preference name="orientation" value="portrait" />
  <preference name="target-device" value="universal" />
  <preference name="fullscreen" value="false" />
    
  <preference name="android-minSdkVersion" value="19"/>
  <preference name="android-targetSdkVersion" value="26"/>
  <preference name="android-installLocation" value="auto"/>
  
  <preference name="debuggable" value="false" />
  
  <plugin name="cordova-plugin-whitelist" spec="1" />
  <plugin name="cordova-plugin-firebase" spec="^2.0.5" />
  <plugin spec="https://github.com/dpa99c/cordova-android-play-services-gradle-release" source="git" />
  
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
      <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
      <allow-intent href="itms:*" />
      <allow-intent href="itms-apps:*" />
  </platform>
</widget>